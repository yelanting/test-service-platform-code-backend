<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper
	namespace="com.testservice.platform.server.dao.TimerTaskPolicyMapper">
	<resultMap id="BaseResultMap"
		type="com.testservice.platform.server.model.TimerTaskPolicy">
		<result column="id" jdbcType="BIGINT" property="id" />
		<result column="cname" jdbcType="VARCHAR" property="cname" />
		<result column="ename" jdbcType="VARCHAR" property="ename" />
		<result column="code" jdbcType="VARCHAR" property="code" />
		<result column="description" jdbcType="VARCHAR"
			property="description" />
		<result column="create_time" jdbcType="VARCHAR"
			property="createTime" />
		<result column="last_update_time" jdbcType="VARCHAR"
			property="lastUpdateTime" />
		<result column="create_by" jdbcType="VARCHAR"
			property="createBy" />
		<result column="last_update_by" jdbcType="VARCHAR"
			property="lastUpdateBy" />
	</resultMap>
	<sql id="Base_Column_List">
		id,
		cname,
		ename,
		code ,
		description,
		create_time,
		last_update_time,
		create_by,
		last_update_by
	</sql>
	<select id="selectByPrimaryKey" parameterType="java.lang.Long"
		resultMap="BaseResultMap">
		select
		<include refid="Base_Column_List" />
		from timer_task_policy
		where id = #{id,jdbcType=BIGINT}
	</select>

	<select id="findTimerTaskPoliciesByCname"
		parameterType="java.lang.String" resultMap="BaseResultMap">
		select
		<include refid="Base_Column_List" />
		from timer_task_policy
		where cname = #{cname}
	</select>

	<select id="findTimerTaskPoliciesByEname"
		parameterType="java.lang.String" resultMap="BaseResultMap">
		select
		<include refid="Base_Column_List" />
		from timer_task_policy
		where ename = #{ename}
	</select>
	<select id="findTimerTaskPoliciesByCnameLike"
		parameterType="java.lang.String" resultMap="BaseResultMap">
		select
		<include refid="Base_Column_List" />
		from timer_task_policy
		where cname like
		CONCAT(#{cname},'%')
	</select>
	<select id="findAll" parameterType="java.lang.String"
		resultMap="BaseResultMap">
		select
		<include refid="Base_Column_List" />
		from timer_task_policy
	</select>
	<select id="findWithEnameExceptThisId" resultMap="BaseResultMap">
		select
		<include refid="Base_Column_List" />
		from timer_task_policy where ename = #{ename} and
		id !=#{id};
	</select>

	<select id="findWithCnameExceptThisId" resultMap="BaseResultMap">
		select
		<include refid="Base_Column_List" />
		from timer_task_policy where cname = #{cname} and
		id !=#{id};
	</select>

	<delete id="deleteByIds" parameterType="java.lang.Long">
		delete from
		timer_task_policy
		where id in
		<foreach item="id" collection="array" open="(" separator=","
			close=")">
			#{id,jdbcType=BIGINT}
		</foreach>
	</delete>
	<select id="selectByIds" parameterType="java.lang.Long"
		resultMap="BaseResultMap">
		select
		<include refid="Base_Column_List" />
		from
		timer_task_policy
		where id in
		<foreach item="id" collection="array" open="(" separator=","
			close=")">
			#{id,jdbcType=BIGINT}
		</foreach>
	</select>
	<delete id="deleteByPrimaryKey" parameterType="java.lang.Long">
		delete from
		timer_task_policy
		where id = #{id,jdbcType=BIGINT}
	</delete>
	<update id="updateByPrimaryKey"
		parameterType="com.testservice.platform.server.model.TimerTaskPolicy">
		update timer_task_policy
		set
		cname =
		#{cname,jdbcType=VARCHAR},
		ename = #{ename,jdbcType=VARCHAR},
		code =
		#{code,jdbcType=VARCHAR},
		description =
		#{description,jdbcType=VARCHAR},
		create_by =
		#{createBy,jdbcType=VARCHAR},
		create_time =
		#{createTime,jdbcType=TIMESTAMP},
		last_update_by =
		#{lastUpdateBy,jdbcType=VARCHAR},
		last_update_time =
		CURRENT_TIMESTAMP(),
		remarks =
		#{remarks,jdbcType=VARCHAR},
		del_flag =
		#{delFlag,jdbcType=TINYINT}
		where id =
		#{id,jdbcType=BIGINT}
	</update>
	<insert id="insert" useGeneratedKeys="true" keyProperty="id"
		parameterType="com.testservice.platform.server.model.TimerTaskPolicy">
		insert into timer_task_policy (id,
		cname,
		ename,
		code,
		description,
		create_time,
		create_by)
		values
		(#{id,jdbcType=BIGINT},
		#{cname,jdbcType=VARCHAR},
		#{ename,jdbcType=VARCHAR},
		#{code,jdbcType=VARCHAR},
		#{description,jdbcType=VARCHAR},
		CURRENT_TIMESTAMP(),
		#{createBy,jdbcType=VARCHAR})
	</insert>

	<select id="findPageByCname" parameterType="java.lang.String"
		resultMap="BaseResultMap">
		<bind name="pattern" value="'%' + _parameter.cname + '%'" />
		select
		<include refid="Base_Column_List" />
		from timer_task_policy
		where cname like #{pattern}
	</select>
	<select id="findPage" parameterType="java.lang.String"
		resultMap="BaseResultMap">
		select
		<include refid="Base_Column_List" />
		from timer_task_policy
	</select>
</mapper>